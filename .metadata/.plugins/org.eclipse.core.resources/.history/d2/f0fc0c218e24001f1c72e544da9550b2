package com.tw.LinkList;

import java.util.LinkedList;
import java.util.List;

public class Link {

	public static void main(String[] args) {
		 List<String> bike = new LinkedList<String>();
		 bike.add("Spotr bike");
		 bike.add("R15");
		 bike.add("Hai abuza");
		 System.out.println(bike);
		 
		 bike.remove(0);
		 System.out.println(bike);
		 
		 bike.set(0, "New bike");
		 System.out.println(bike);
		 
		 bike.toArray();
		 System.out.println(bike);
		 

		
		 
	}

}
