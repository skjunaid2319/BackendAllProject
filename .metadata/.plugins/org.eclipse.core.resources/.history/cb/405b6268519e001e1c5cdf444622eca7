package com.tw.datatype;

public class Arithematicoperator {

	public static void main(String[] args) {
		// simple operators
	
		// Arithmetic operators 
		
		int a=10;
		int b=20;
		
		int plus=a+b;
		System.out.println("by junaid"+plus);
		
		
		int minus=a-b;
		System.out.println("by junaid"+minus);

	}

}
